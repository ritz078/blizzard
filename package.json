{
  "name": "snape",
  "version": "0.3.1",
  "description": "A torrent client to search, stream and download torrents. Play music, view images and stream videos.",
  "engines": {
    "node": ">=9"
  },
  "main": "main/index.js",
  "author": "Ritesh Kumar",
  "scripts": {
    "start": "electron --inspect main",
    "build": "next build renderer && next export -o build renderer",
    "precommit": "lint-staged",
    "format": "prettier-eslint --write main/**/*.js renderer/**/*.js",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "pack": "npm run build && electron-builder --dir",
    "dist": "rm -rf dist && npm run build && electron-builder -p always",
    "lint": "eslint main/**/*.js renderer/**/*.js",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritz078/snape.git"
  },
  "keywords": [
    "torrent",
    "webtorrent",
    "stream",
    "download",
    "video"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/snape/issues"
  },
  "homepage": "https://github.com/ritz078/snape#readme",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "chai": "^4.1.2",
    "chromecasts": "^1.9.1",
    "downloads-folder": "^1.0.1",
    "electron-debug": "^1.5.0",
    "electron-is-dev": "^0.3.0",
    "electron-lets-move": "^0.0.5",
    "express": "^4.16.3",
    "fix-path": "^2.1.0",
    "get-port": "^3.2.0",
    "internal-ip": "^3.0.1",
    "is-electron-renderer": "^2.0.1",
    "lodash": "^4.17.13",
    "mime": "^2.2.2",
    "node-notifier": "^5.2.1",
    "parse-torrent": "^5.8.3",
    "pretty-bytes": "^4.0.2",
    "protobufjs": "^6.8.6",
    "pump": "^3.0.0",
    "query-string": "^6.0.0",
    "range-parser": "^1.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "snape-config": "^0.0.1",
    "tempy": "^0.2.1",
    "thepiratebay": "^1.4.7",
    "url-loader": "^1.0.1",
    "vlc-command": "^1.1.1",
    "webtorrent": "^0.98.24",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babili-webpack-plugin": "^0.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "devtron": "^1.4.0",
    "electron": "1.8.4",
    "electron-builder": "^20.8.1",
    "electron-builder-squirrel-windows": "^20.8.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.0.1",
    "lodash-webpack-plugin": "^0.11.4",
    "mdi": "1.9.33",
    "next": "^5.1.0",
    "next-redux-wrapper": "^1.2.0",
    "nprogress": "^0.2.0",
    "plyr": "^3.0.11",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.3",
    "postcss-url": "^7.3.1",
    "prettier-eslint-cli": "^4.7.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "rc-slider": "^8.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ink": "^6.2.0",
    "react-lazy-card": "^0.1.8",
    "react-loading-bar": "^0.0.7",
    "react-modal": "^3.3.2",
    "react-notification-badge": "^1.3.4",
    "react-photostory": "^0.7.2",
    "react-redux": "^5.0.7",
    "react-toastify": "^1.7.0",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.8",
    "spectre.css": "^0.2.14",
    "styled-components": "^3.2.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "build": {
    "appId": "com.riteshkr.snape",
    "asar": false,
    "win": {
      "target": [
        "squirrel"
      ],
      "icon": "renderer/static/images/snape.ico"
    },
    "squirrelWindows": {},
    "mac": {
      "category": "public.app-category.developer-tools",
      "extendInfo": {
        "LSUIElement": 1
      },
      "icon": "renderer/static/images/snape.icns"
    },
    "files": [
      "main",
      "node_modules",
      "build"
    ]
  }
}
